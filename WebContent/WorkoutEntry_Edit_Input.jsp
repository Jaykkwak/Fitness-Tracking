	width: 70%;
}

.middle {
	display: grid;
	grid-template-columns: 1.5fr 1fr;
	margin: 0 auto;
	width: 90%;
}

.middlepart {
	height: 80%;
	margin-left: 50px;
}

.body {
	font-size: 3rem;
	font-weight: 800;
}

.middle #date {
	font-size: 2.3rem;
	color: #b3d6a4;
	margin-bottom: 20px;
}

#table0 {
